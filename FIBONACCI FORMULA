
 ///Fibonacci nth term calculated using formula :
/*
    Fn = {[(root5  +1)/2] ^n} / root5
*/
///valid till n=92. 
///SC:O(1) TC:O(log N)
#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cout<<"Enter the value of n: ";
    cin>>n;
    double phi= (1+sqrt(5))/2;
    long long int ans= round(pow(phi,n)/sqrt(5));
    cout<<n<<"th Fibonnaci number is : "<<ans;    
}

